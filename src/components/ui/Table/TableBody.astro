---
/**
 * TableBody - Corps du tableau
 * 
 * Wraps <tbody> pour les lignes de donn√©es.
 * 
 * @param {string} className - Classes CSS additionnelles
 */

interface Props {
  className?: string;
}

const {
  className = '',
} = Astro.props;

const classes = [
  'table-body',
  className,
].filter(Boolean).join(' ');
---

<tbody class={classes}>
  <slot />
</tbody>
